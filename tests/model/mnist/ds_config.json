{
  "train_batch_size": 1,
  "gradient_accumulation_steps": 1,
  "tensorboard": {
    "enabled": true,
    "output_path": "./tb",
    "job_name": "mnist_ds"
  },
  "debugger": {
    "enable": false,
    "local_path": "./db",
    "export_tensorboard": true,
    "hook_parameters": {
      "save_all": true,
      "collections": "weights, gradients, biases, inputs, outputs",
      "reductions": "max, mean, variance",
      "save_steps": "0,1,2,3",
      "save_interval": 10
    }
  }
}
